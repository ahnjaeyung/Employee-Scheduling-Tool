<div class="container">
<div class="mt-5">
  {{>calendar}}
</div>


<table class="table table-striped table-dark mt-5" id="employeeTable">
  <thead>
    <tr>
      <th scope="col">First</th>
      <th scope="col">Last</th>
      <th scope="col" id="1">Day 1 Availability</th>
       <th scope="col" id="2">Day 2 Availability</th>
      <th scope="col" id="3">Day 3 Availability</th>
      <th scope="col" id="4">Day 4 Availability</th>
      <th scope="col" id="5">Day 5 Availability</th>
      <th scope="col" id="6">Day 6 Availability</th>
      <th scope="col" id="7">Day 7 Availability</th>
      <th scope="col" id="8">Day 8 Availability</th>
      <th scope="col" id="9">Day 9 Availability</th>
      <th scope="col" id="10">Day 10 Availability</th>
      <th scope="col" id="11">Day 11 Availability</th>
      <th scope="col" id="12">Day 12 Availability</th>
      <th scope="col" id="13">Day 13 Availability</th>
      <th scope="col" id="14">Day 14 Availability</th>
    
    </tr>
  </thead>
  <tbody>
    
{{#each employees as |emp|}}
{{> employees}}
{{/each}}
    
  </tbody>
</table>
</div>

<script src="./js/dateMaker.js"></script>